<%= form_for([@todo_list, @todo_list.todo_items.build]) do |f| %>
 <%= f.text_field :content, placeholder: "Task" %>
 <p>
 	Priority:
 	<%= f.select :priority, options_for_select([["High", "h"], ["Medium", "m"],["Low", "l"]]) %>
 <p>
 	Deadline date:
 	<%= f.date_field :deadline, data:{provide:'datepicker'} %>
 <p>
 <%= f.submit %>
<% end %>